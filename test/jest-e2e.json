{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "reporters": [ "default", "jest-junit" ],
  "globals": {
    "ts-jest": {
      "astTransformers": {
        "before": ["test/next-jest-graphql.ts"]
      }
    }
  }
}
